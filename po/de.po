msgid " usage: %s [-A] [-Ddebuglevel] (jobid|user|'all')* [printer]\n"
msgstr ""

#: src/common/lprm.c:339 src/common/lprm.c:355
msgid "  -A           - use authentication\n"
msgstr "  -A           - nutze Authentifikation\n"

#: src/common/lprm.c:341 src/common/lprm.c:360
msgid "  user           removes user jobs\n"
msgstr "  USER           lösche Jobs von USER \n"

#: src/common/lprm.c:342 src/common/lprm.c:361
msgid "  all            removes all jobs\n"
msgstr "  all            lösche alle Jobs\n"

#: src/common/lprm.c:343 src/common/lprm.c:362
msgid "  jobid          removes job number jobid\n"
msgstr "  jobid          lösche Job mit Nummer jobid\n"

#: src/common/lprm.c:344 src/common/lprm.c:363
msgid " Example:\n"
msgstr " Beispiel:\n"

#: src/common/lprm.c:345
msgid "    'clean 30 lp' removes job 30 on printer lp\n"
msgstr "    'clean 30 lp' lösche Job 30 für Drucker lp\n"

#: src/common/lprm.c:346
msgid "    'clean'       removes first job on default printer\n"
msgstr "    'clean'       lösche den ersten Job für den Standarddrucker\n"

#: src/common/lprm.c:347
msgid "    'clean all'      removes all your jobs on default printer\n"
msgstr "    'clean all'      lösche alle Jobs für den Standarddrucker\n"

#: src/common/lprm.c:348
msgid "    'clean all all'  removes all your jobs on all printers\n"
msgstr "    'clean all all'  lösche alle Jobs für alle Drucker\n"

#: src/common/lprm.c:349
msgid "  Note: clean removes only jobs for which you have removal permission\n"
msgstr "  Hinweis: Man kann nur Jobs löschen wenn man das Recht dazu hat\n"

