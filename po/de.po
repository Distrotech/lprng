msgstr "    'lprm -a'      alle Jobs für alle Drucker löschen\n"

#: src/common/lprm.c:366
msgid "    'lprm -a all'  removes all jobs on all printers\n"
msgstr "    'lprm -a all'  alle Jobs für alle Drucker löschen\n"

#: src/common/lprm.c:367
