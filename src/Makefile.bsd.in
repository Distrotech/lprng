sclient: sclient.o liblpr.a
	$(LDCC) $(LDFLAGS) -o $@ $> $(LIBS)

cancel:
	rm -f $@; ln -s lprm $@ 
lp: lpr
	rm -f $@; ln -s lpr $@

###########################################################################
# lpd.conf

../lpd.conf: Makefile vars.c
	rm -f $@
	$(CPP) $(CFLAGS) -C ${SRC}/common/vars.c | /bin/sh ${SRC}/../UTILS/make_lpd_conf $> >$@.tmp
	mv $@.tmp $@
	chmod 644 $@

